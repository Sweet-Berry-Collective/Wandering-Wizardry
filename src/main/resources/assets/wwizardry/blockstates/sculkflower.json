{
  "multipart": [
    {
      "apply": {
        "model": "wwizardry:block/sculkflower"
      },
      "when": {
        "sculked": "false"
      }
    },
    {
      "apply": {
        "model": "wwizardry:block/sculkflower_sculk"
      },
      "when": {
        "sculked": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/sculk_vein",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "sculked": "true",
        "sculk_below": "false"
      }
    }
  ]
}
